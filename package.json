  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romancircus/opencode-kimi-auth.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "opencode",
    "kimi",
    "auth",
    "plugin",
    "authentication"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
