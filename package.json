    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@openauthjs/openauth": "^0.3.0"
  },
  "devDependencies": {
    "@opencode-ai/plugin": "^0.4.45",
    "@types/node": "^20.0.0",
    "typescript": "^5.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romancircus/opencode-kimi-auth.git"
  },
  "bugs": {
    "url": "https://github.com/romancircus/opencode-kimi-auth/issues"
  },
  "homepage": "https://github.com/romancircus/opencode-kimi-auth#readme",
  "license": "Apache-2.0",
  "keywords": [
    "opencode",
    "kimi",
    "auth",
    "plugin",
    "authentication",
    "oauth",
    "moonshot",
    "ai",
    "mcp"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Roman Circus Studio",
