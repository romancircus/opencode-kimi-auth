{
  "name": "opencode-kimi-auth",
  "version": "0.1.1",
  "description": "OpenCode authentication plugin for Kimi models - enables OAuth Device Authorization flow",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "example-opencode.json"
  ],
  "scripts": {
